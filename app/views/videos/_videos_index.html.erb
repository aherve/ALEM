<% @videos.each do |v| %>
  <div class="well row">
    <div class="col-md-6">
      <h3><%= v.title %></h3>
      <iframe width="420" height="315" src="http://www.youtube.com/embed/<%= v.youtube_id %>" frameborder="0" allowfullscreen></iframe>
    </div>
    <div class="col-md-6"> <%= describe(v) %> </div>
  </div>
<% end %>
